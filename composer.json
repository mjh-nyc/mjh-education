{
    "name": "roots/bedrock",
    "type": "project",
    "license": "MIT",
    "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
    "homepage": "https://roots.io/bedrock/",
    "authors": [
        {
            "name": "Scott Walkinshaw",
            "email": "scott.walkinshaw@gmail.com",
            "homepage": "https://github.com/swalkinshaw"
        },
        {
            "name": "Ben Word",
            "email": "ben@benword.com",
            "homepage": "https://github.com/retlehs"
        }
    ],
    "keywords": [
        "bedrock",
        "roots",
        "wordpress",
        "stack",
        "composer",
        "vagrant",
        "wp"
    ],
    "support": {
        "issues": "https://github.com/roots/bedrock/issues",
        "forum": "https://discourse.roots.io/category/bedrock"
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "johnpbloch/wordpress-core-installer": true,
            "philippbaschke/acf-pro-installer": true,
            "ffraenz/private-composer-installer": true,
            "roots/wordpress-core-installer": true
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "1":{
            "type": "composer",
            "url": "https://connect.advancedcustomfields.com"
        },
        "2": {
            "type": "package",
            "package": {
                "name": "deliciousbrains/wp-offload-s3-pro",
                "version": "1.5.1",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://deliciousbrains.com/dl/wp-offload-s3-pro-latest.zip?licence_key=1af4b9aa-71f7-4036-a5ef-300592795795&site_url=mjhnyc.org"
                }
            }
        },
        "3": {
            "type": "package",
            "package": {
                "name": "mjh-nyc/mjhedu-custom-configurations",
                "version": "dev-master",
                "type": "wordpress-muplugin",
                "source": {
                    "type": "git",
                    "url": "https://github.com/mjh-nyc/mjhedu-custom-configurations.git",
                    "reference": "1.4.3"
                }
            }
        },
        "my-yoast": {
            "type": "composer",
            "url": "https://my.yoast.com/packages/"
        }
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.2",
        "vlucas/phpdotenv": "^5.5",
        "oscarotero/env": "^2.1",
        "roots/wp-password-bcrypt": "^1.0.0",
        "roots/wp-config": "1.0.0",
        "mjh-nyc/mjhedu-custom-configurations": "dev-master",
        "wpackagist-plugin/advanced-forms": "^1.0",
        "deliciousbrains/wp-offload-s3-pro": "^1.5",
        "wpackagist-plugin/wp-super-cache": "^1.6.4",
        "yoast/wordpress-seo-premium": "^18.0",
        "wpackagist-plugin/wps-hide-login": "^1.8",
        "wpackagist-plugin/export-wp-users-xml-csv": "^1.0",
        "wpackagist-plugin/wp-all-export": "^1.3",
        "roots/wordpress": "^6.2",
        "roots/bedrock-autoloader": "^1.0",
        "roots/bedrock-disallow-indexing": "^2.0",
        "wpengine/advanced-custom-fields-pro": "^6.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0.2",
        "roave/security-advisories": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": [
            "phpcs"
        ]
    }
}
